print 'While loop test.';

let x = 2;
let y = 2;

while y <= x {
	x += 250;
	y *= 2;

	if (x > 1000) && (x < 1200) {
		print `x: {x}; y: {y}; continuing`;
		continue;
	}
	else if x > 1500 {
		print `x: {x}; y: {y}; breaking early`;
		break;
	}

	print `x: {x}; y: {y}`;

	if y > x {
		print 'y is now greater than x.';
	}
}

print 'Done.';